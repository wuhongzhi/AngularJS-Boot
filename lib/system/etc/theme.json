{
    "depends":
    {
        "paths":
        {
            "angular-touch": "lib/angular.js/angular-touch",
            "ui-bootstrap": "lib/angular-bootstrap/ui-bootstrap-tpls",
            "ui-material": "lib/angular-material/angular-material"
        },
        "shim":
        {
            "angular-touch": ["angular"],
            "ui-bootstrap": [
                "angular-touch",
                "angular-sanitize",
                "style!css/themes/bootstrap/bootstrap",
                "style!css/themes/bootstrap/bootstrap-theme",
                "style!css/shared/icons/awesome/font-awesome"
            ],
            "ui-material": [
                "angular-sanitize",
                "style!css/themes/material/angular-material",
                "style!css/themes/material/angular-material-icons"
            ]
        }
    },
    "themes":
    {
        "material":
        {
            "depends": [
                "ngMaterial"
            ],
            "modules": [
                "ui-material"
            ],
            "flex": true
        },
        "bootstrap":
        {
            "depends": [
                "ui.bootstrap", "ngTouch"
            ],
            "modules": [
                "ui-bootstrap"
            ]
        }
    }
}